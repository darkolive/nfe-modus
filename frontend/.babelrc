{
  "presets": [
    ["next/babel", {
      "preset-react": {
        "runtime": "automatic",
        "importSource": "react"
      }
    }]
  ],
  "plugins": [
    ["@babel/plugin-transform-modules-commonjs", {
      "allowTopLevelThis": true,
      "loose": true,
      "strict": false
    }]
  ]
}
